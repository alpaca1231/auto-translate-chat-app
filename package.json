{
  "name": "auto-translate-chat-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:hcm": "yarn hcm -w --log-level=silent",
    "build": "next build",
    "start": "next start",
    "hcm": "hcm 'src/**/*.module.{css,scss}' --arbitraryExtensions",
    "prettier": "prettier --config .prettierrc './**/*.{js,jsx,ts,tsx,json,md}'",
    "format": "yarn prettier -w",
    "lint": "next lint --dir src",
    "lint:fix": "yarn lint --fix",
    "fix": "run-s format lint:fix"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.6.1",
    "@supabase/supabase-js": "^2.21.0",
    "axios": "^1.4.0",
    "iso-639-1": "^2.1.15",
    "next": "^13.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "swr": "^2.1.5",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "eslint": "8.39.0",
    "eslint-config-next": "^13.4.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "happy-css-modules": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8"
  }
}
